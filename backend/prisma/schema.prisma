generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model ScrapRecord {
  id         Int      @id @default(autoincrement())
  machineId  String
  scrapIndex Int
  value      Int
  timestamp  DateTime
  createdAt  DateTime @default(now())

  @@index([machineId, scrapIndex])
  @@index([timestamp])
}
